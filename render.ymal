databases:
  - name: mysitedb
    plan: free
    databaseName: dabaNachan
    user: ore

services:
  - type: web
    plan: free
    name: watasi
    runtime: python
    buildCommand: "./build.sh"
    startCommand: "python -m gunicorn bookproject.asgi:application -kuvicorn.workers.UvicornWorker"
    envVars:
      - key: DATABASE_URL
        fromDatabase:
          name: mysitedb
          property: connectionString
      - key: WEB_CONCURRENCY
        value: 4